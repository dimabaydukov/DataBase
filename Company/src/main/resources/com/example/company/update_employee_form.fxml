        </Group>
        <Group layoutX="46.0" layoutY="233.0">
            <children>
                <Label prefHeight="18.0" prefWidth="73.0" text="Эл. почта" />
                <TextField fx:id="emailUser" layoutX="73.0" layoutY="-4.0" prefHeight="26.0" prefWidth="417.0" />
            </children>
        </Group>
        <Button fx:id="updateButton" layoutX="236.0" layoutY="325.0" mnemonicParsing="false" onAction="#updateOnClick" text="Обновить" />
        <Label fx:id="labelError" layoutX="119.0" layoutY="21.0" textFill="#e72828" />
    </children>

</AnchorPane>
