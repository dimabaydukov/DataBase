<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.company.UpdateEmployeeFormController">
    <children>
        <Group layoutX="46.0" layoutY="53.0">
            <children>
                <Label prefHeight="18.0" prefWidth="73.0" text="Логин" />
