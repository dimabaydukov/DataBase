<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="191.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.company.SignInFormController">

    <children>
        <Group layoutX="46.0" layoutY="53.0">
            <children>
                <Label prefHeight="18.0" prefWidth="73.0" text="Логин" />
                <TextField fx:id="loginUser" layoutX="73.0" layoutY="-4.0" prefHeight="26.0" prefWidth="417.0" />
            </children>
        </Group>
        <Group layoutX="46.0" layoutY="98.0">
            <children>
                <Label prefHeight="18.0" prefWidth="73.0" text="Пароль" />
                <TextField fx:id="passwordUser" layoutX="73.0" layoutY="-4.0" prefHeight="26.0" prefWidth="417.0" />
            </children>
        </Group>
        <Button fx:id="signInButton" layoutX="276.0" layoutY="143.0" mnemonicParsing="false" onAction="#onSignInClick" text="Войти" />
        <Button fx:id="backButton" layoutX="550.0" layoutY="156.0" mnemonicParsing="false" onAction="#onBackClick" prefHeight="20.0" prefWidth="36.0" text="Назад">
            <font>
                <Font size="8.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
